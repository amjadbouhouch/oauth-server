{
  "name": "@oauth/server",
  "version": "1.0.0",
  "main": "/dist/index.js",
  "license": "MIT",
  "scripts": {
    "build": "tsc -p tsconfig.json --outDir ./dist && tsc-alias",
    "dev": "ts-node-dev -r tsconfig-paths/register --inspect --transpile-only --ignore-watch node_modules src/index.ts",
    "production": "node ./dist/server.js",
    "test": "jest --runInBand --detectOpenHandles",
    "test:watch": "jest --watch --runInBand --detectOpenHandles --verbose",
    "lint": "eslint src --max-warnings=0 --fix",
    "typecheck": "tsc --project tsconfig.json --noEmit",
    "tailwind:build": "npx tailwindcss -i ./src/*.css ./src/*.ejs  -o ./src/dist/output.css --watch"
  },
  "dependencies": {
    "bcrypt": "^5.1.0",
    "body-parser": "^1.20.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "cors": "^2.8.5",
    "dotenv": "^8.6.0",
    "ejs": "^3.1.8",
    "express": "^4.18.1",
    "express-async-errors": "^3.1.1",
    "express-session": "^1.17.3",
    "http-proxy-middleware": "^2.0.6",
    "inversify": "^6.0.1",
    "inversify-express-utils": "^6.4.3",
    "jose": "^4.11.2",
    "jsonwebtoken": "^9.0.0",
    "memorystore": "^1.6.7",
    "morgan": "^1.10.0",
    "nanoid": "3.3.4",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.3",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.15",
    "@types/express-session": "^1.17.5",
    "@types/jest": "28.1.8",
    "@types/jsonwebtoken": "^9.0.0",
    "@types/node": "^14.14.14",
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@typescript-eslint/parser": "^5.41.0",
    "autoprefixer": "^10.4.13",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.1.4",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "29.0.3",
    "jest-mock-extended": "2.0.7",
    "postcss": "^8.4.21",
    "prettier": "^2.7.1",
    "resolve-tspaths": "^0.7.1",
    "tailwindcss": "^3.2.4",
    "ts-guard-clause": "0.0.7",
    "ts-jest": "29.0.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^1.1.1",
    "tsc-alias": "^1.7.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.8.2"
  }
}
